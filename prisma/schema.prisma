generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id() @default(uuid())
  name      String
  username  String
  password  String
  permition String
  avatar    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()
}
